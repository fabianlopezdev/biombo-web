{
  "name": "astro-frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:prod-data": "cross-env SANITY_DATASET=production astro dev",
    "build": "astro build",
    "build:production": "cross-env NODE_ENV=production astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.ts,.astro,.json",
    "lint:fix": "eslint . --ext .js,.ts,.astro,.json --fix",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "check-format": "prettier --check . --plugin=prettier-plugin-astro",
    "test": "vitest run",
    "test:watch": "vitest",
    "i18n:extract": "astro-i18n extract",
    "i18n:generate:pages": "astro-i18n generate:pages --purge",
    "i18n:generate:types": "astro-i18n generate:types",
    "i18n:sync": "npm run i18n:generate:pages && npm run i18n:generate:types"
  },
  "dependencies": {
    "@astrojs/netlify": "^6.5.9",
    "@portabletext/to-html": "^2.0.14",
    "@portabletext/types": "^2.0.13",
    "@sanity/astro": "^3.2.6",
    "@sanity/client": "^7.2.2",
    "astro": "^5.13.3",
    "astro-i18n": "^2.2.4",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.32.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jsonc-eslint-parser": "^2.4.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.4"
  }
}
